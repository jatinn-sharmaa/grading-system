#include <stdio.h>
int main(void){
int num;
printf("Enter your mark :-  ");
scanf("%d",&num);
printf("\nYou entered %d ", num); // printing outputs

  if(num >= 85){
  printf(" \nYou got A grade"); // printing outputs
    }
  else if ( num >=70){ // Note the space between else & if
    printf(" \nYou got B grade");
    }
  else if ( num >=55){
    printf(" \nYou got C grade");
    }
  else if ( num >=40){
    printf("\n You got D grade");
    }
  else if (num <40){
    printf("\nYou got F grade ");
  }
return 0;
}
